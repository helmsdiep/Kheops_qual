//IR0020
function fn01AC0020_Init(hmGlobals,item, en) {
	var strItemType = getScriptByPath("/scripts/triggers/LG.Library.LGUtils").getFunctionByName("getItemLevel").invoke(item);
	if(strItemType!=null && (strItemType=="3" || strItemType=="4")) {
		var strCtgName;
		if(substring(item.getCatalog().getCtgName(),0,2)=="CA") {
			strCtgName = getColAreaByName(item.getCatalog().getCtgName()).getColAreaSrcContainer().getCtgName();
		}
		else {
			strCtgName = item.getCatalog().getCtgName();
		}
		if(strCtgName!=null && strCtgName!="") {
			var strCatalogCode = substring(strCtgName,0,4);
			if(strCatalogCode=="C010" || strCatalogCode=="C800") {
				en.setEntryNodeValue("[PALLET] - Palette homogène standard");
			}
		}
	}
	else if (strItemType=="2") {
		en.setEntryNodeValue("[BASE_UNIT_OR_EACH] - Unité consommateur / Unité de base");
	}	
}

