//IR0360
function fn06DN0360_Init(hmGlobals,item, en) {
	var strCtgName;
	if(substring(item.getCatalog().getCtgName(),0,2)=="CA") {
		strCtgName = getColAreaByName(item.getCatalog().getCtgName()).getColAreaSrcContainer().getCtgName();
	}
	else {
		strCtgName = item.getCatalog().getCtgName();
	}
	if(strCtgName!=null && strCtgName!="") {
		var strCatalogCode = substring(strCtgName,0,4);
		var strItemType = getScriptByPath("/scripts/triggers/LG.Library.LGUtils").getFunctionByName("getItemLevel").invoke(item);
		if(strCatalogCode=="C800" && (strItemType=="3" || strItemType=="4")) {
			var iPlaceOfStorageGroupLen = 0;
			var oPlaceOfStorageGroup = getEn(hmGlobals,item,"PlaceOfStorageGroup");
			if (oPlaceOfStorageGroup!=null) {
				iPlaceOfStorageGroupLen=oPlaceOfStorageGroup.size();
			}
			//
			if(iPlaceOfStorageGroupLen==1){
				en.setEntryNodeValue(1);
			}
		}
		else {
			var iGrossWeight = getValue(hmGlobals,item,"04CP0215-grossWeight");
			if(iGrossWeight!=null) {
				en.setEntryNodeValue(iGrossWeight);
			}
		}
	}
}

//IR0361 lookup table 210 colonne C01-DisplayName
function fn06DN0361_Init(hmGlobals,item, en) {
	var sVal=getL210Unit("KGM");
	var PlaceOfStorageGroup = getEn(hmGlobals,item,"PlaceOfStorageGroup");
	var i;
	for(i=0;i<PlaceOfStorageGroup.size();i++) {
		var lkpGrossWeightUnit = getEn(hmGlobals,item,"PlaceOfStorageGroup#"+i+"/06DN0361-GrossWeightUnit");
		if(lkpGrossWeightUnit!=null) {
			item.setEntryAttrib(lkpGrossWeightUnit[0].getEntryNodeExactPath(),sVal);
		}
	}
}

