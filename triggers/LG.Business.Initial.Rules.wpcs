//IR0097 lookup table 210 colonne C01-DisplayName
function fn04CP0097_Init(hmGlobals,item, en) {
	var strCatalogCode = concat("C",substring(item.getCatalog().getCtgName(),2,5));
	var strItemType = getScriptByPath("/scripts/triggers/LG.Library.LGUtils").getFunctionByName("getItemLevel").invoke(item);
	if(strCatalogCode=="C110") {
		if(strItemType!=null && strItemType=="2") {
			en.setEntryNodeValue(getL210Unit("MMT"));
		}
		else if (strItemType!=null && (strItemType=="3" || strItemType=="4")) {
			en.setEntryNodeValue(getL210Unit("MTR"));
		}
	}
	else if(strCatalogCode=="C020") {
		if (strItemType!=null && (strItemType=="3" || strItemType=="4")) {
			en.setEntryNodeValue(getL210Unit("MTR"));
		}
	}
	else if(strCatalogCode=="C010") {
		if(strItemType!=null && strItemType=="2") {
			en.setEntryNodeValue(getL210Unit("CMT"));
		}
		else if (strItemType!=null && (strItemType=="3" || strItemType=="4")) {
			en.setEntryNodeValue(getL210Unit("MTR"));
		}
	}
}

