// IR0775
function fn01AC0775_Init(hmGlobals,item,en) {
	var strCtgName;
	if(substring(item.getCatalog().getCtgName(),0,2)=="CA") {
		strCtgName = getColAreaByName(item.getCatalog().getCtgName()).getColAreaSrcContainer().getCtgName();
	}
	else {
		strCtgName = item.getCatalog().getCtgName();
	}
	if(strCtgName!=null && strCtgName!="") {
		var strCatalogCode = substring(strCtgName,0,4);
		var oL907LkpTable = getLkpByName("L907-BUnitSAPRelevance");
		if(strCatalogCode!=null && strCatalogCode!="" && strCatalogCode!="null") {
			var L907Values=lookupValues(oL907LkpTable,strCatalogCode);
			var iItemType = getScriptByPath("/scripts/triggers/LG.Library.LGUtils").getFunctionByName("getItemLevel").invoke(item);
			//
			var boolSAPRelevanceBUnit = L907Values[1];				
			var boolSAPRelevancePItem1 = L907Values[2];
			var boolSAPRelevancePItem2 = L907Values[3];
			var boolSAPRelevanceProd= L907Values[4];
			//
			if(iItemType==1) {
				item.setEntryAttrib(en.getEntryNodePath(), boolSAPRelevanceProd);
			}
			else if (iItemType==2) {
				item.setEntryAttrib(en.getEntryNodePath(), boolSAPRelevanceBUnit);
			}
			else if (iItemType==3) {
				item.setEntryAttrib(en.getEntryNodePath(), boolSAPRelevancePItem1);
			}				
			else if (iItemType==4) {
				item.setEntryAttrib(en.getEntryNodePath(), boolSAPRelevancePItem2);
			}
		}
	}
}

